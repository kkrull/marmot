# https://editorconfig.org
root = false

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_size = 2
indent_style = space
trim_trailing_whitespace = false

[LICENSE]
indent_size = off

[*.md]
indent_size = off
max_line_length = off
trim_trailing_whitespace = false

[*.sh]
quote_type = single
